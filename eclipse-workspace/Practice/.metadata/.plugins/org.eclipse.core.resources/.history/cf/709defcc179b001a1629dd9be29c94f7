package HUAWEITest;

import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String ip1="";
		String ip2="";
		String ip3="";
		int flag=0;
		while(sc.hasNextLine()) {
			if(flag==0) {
				ip1=sc.nextLine();
			}else if(flag==1) {
				ip2=sc.nextLine();
			}else {
				ip3=sc.nextLine();
			}
			flag++;
		}
		
		
		
	};
}
